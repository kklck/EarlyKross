<!-- Widget: Roster Changelog -->
<aside class="widget card widget--sidebar widget-newslog">
	<div class="widget__title card__header">
		<h4>Roster Changelog</h4>
	</div>
	<div class="widget__content card__content">
		<ul class="newslog">
			{{#each football.sp.team.changelog}}
			<li class="newslog__item newslog__item--{{type}}">
				<div class="newslog__item-inner">
					<div class="newslog__content">{{{content}}}</div>
					<time class="newslog__date" datetime="2016-08-24">{{date}}</time>
				</div>
			</li>
			{{/each}}
		</ul>
	</div>
</aside>
<!-- Widget: Roster Changelog / End -->
